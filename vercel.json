{
  "version": 2,
  "routes": [
    { "src": "/api/v1/token", "dest": "/index.js" },
    { "src": "/api/v1/query-transaction-status", "dest": "/index.js" },
    { "src": "/api/v1/sendmail", "dest": "/index.js" },
    { "src": "/api/v1/redirect", "dest": "/index.js" }
  ],
  "headers": [
    {
      "source": "/api/v1/token",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "http://localhost:4200, https://khoka-dev.web.app/, https://khoka.co"
        },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" },
        { "key": "Access-Control-Allow-Credentials", "value": "true" }
      ]
    },
    {
      "source": "/api/v1/redirect",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "http://localhost:4200, https://khoka.co"
        },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" },
        { "key": "Access-Control-Allow-Credentials", "value": "true" }
      ]
    },
    {
      "source": "/api/v1/sendmail",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" },
        { "key": "Access-Control-Allow-Credentials", "value": "true" }
      ]
    }
  ]
}