{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "OPTION"
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/api/v1/token",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "OPTION"
            ]
        },
        {
            "source": "/api/v1/query-transaction-status",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "OPTION"
            ]
        }
    ]
}