{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
           "destination":"index.js"
        },
        {
            "source": "/api/v1/token",
            "destination":"index.js"
        },
        {
            "source": "/api/v1/query-transaction-status",
            "destination":"index.js"
        }
    ]
}